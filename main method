package com.ganesh.java;

public class BasicProgram implements Loop
{

int x,y;


public static void main(String[] args)

{

BasicProgram obj= new BasicProgram();
obj.sum(10,20);

BasicProgram.sub(30,10);

Loop l= ()-> System.out.println("This is the implementation of Loop Interface");
l.method();

}


public void sum(int a, int b)
{


System.out.println("Addition of two number is ");
System.out.println(a+b);


}

public static void sub(int a, int b)
{

System.out.println("Subtraction of two numbers is");
System.out.println(a-b);

}


}

interface Loop
{

public abstract method();


}

class ChildClass
{
public void overrideMethod()
{
System.out.pritln("Over riding Method");

}


}
